var output=[];

var count=1;

function fizzBuzz(){
     if(count%15===0){
        output.push("FizzBuzz");
         }
     else if(count%3===0){
        output.push("Fizz");
        } 
     else if(count%5===0){
      output.push("Buzz");
      }
      else{
     output.push(count);
      }
count++;

console.log(output);
}

fizzBuzz();

////////output////////
(20)Â [1, 2, "Fizz", 4, "Buzz", "Fizz", 7, 8, "Fizz", "Buzz", 11, "Fizz", 13, 14, "FizzBuzz", 16, 17, "Fizz", 19, "Buzz"]
